        <member name="M:ProjectADApi.Controllers.V1.AccountController.Login(ProjectADApi.Contract.V1.Request.LoginRequest)">
             <summary>
             Use login, authenticat registered users on the plarform
             </summary>
             
             <remarks>
             Sample request:
             
                 POST api/Account/Login
                 
                 {        
                   "Username": "info@bluecollar.com.ng",
                   "Password": "@infoBlue1"
                 }
                 
             </remarks>
             
             <param name="model"></param>  
            
        </member>
        <member name="M:ProjectADApi.Controllers.V1.AccountController.Register(ProjectADApi.Contract.V1.Request.CreateUserRequest)">
             <summary>
             
             Register a new user on the platform
             Password must have at least one special character,
             one upper case character and
             a digit between 0-9
             
             </summary>
             
             <remarks>
             Sample request:
             
                 POST api/Account/Register
                 
                 {
                   "EmailAddress": "info@bluecollar.com.ng",
                   "Password": "@infoBlue1",
                   "CreationDate": 2020/2/10,
                   "RoleId": 1,
                   "UserName": "BlueCollarHub"
                 }
                 
             </remarks>
             
             <param name="model"></param>  
            
        </member>
        <member name="M:ProjectADApi.Controllers.V1.AccountController.AllUserLogin(System.Nullable{System.Int32})">
             <summary>
             
             Display all registered user/login details on the platofrm
             
             </summary>
             
             <remarks>
             Sample request:
             
                 GET api/Account/AllUserLogin
                 
             </remarks>
             
             <response code="201">Returns all Registered users</response>
             <response code="204">Return no content found </response>
            
        </member>
        <member name="M:ProjectADApi.Controllers.V1.AccountController.ForgotPassword(ProjectADApi.Contract.V1.Request.ForgotPasswordRequest)">
             <summary>
             
               Initiate a password reset when a user forget his/her password
             
             </summary>
             
             <remarks>
             Sample request:
             
                 POST api/Account/ForgotPassword
                 
                 {
                     "Email" : "User registered email"
                 }
                 
             </remarks>
                     
            
        </member>
